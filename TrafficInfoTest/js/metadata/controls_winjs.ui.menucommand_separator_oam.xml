<?xml version="1.0" encoding="utf-8"?>
<widget xmlns="http://openajax.org/metadata"
        xmlns:win="http://microsoft.com/blend/5/namespace"
        spec="1.0"
        id="http://www.microsoft.com/schemas/corsica/1.0/MenuCommand_Separator"
        name="WinJS.UI.MenuCommand_Separator"
        version="1.0">
  <author name="Microsoft Corporation" />
  <title type="text/plain">Menu Command Separator</title>
  <icons>
    <icon src="controls_winjs.ui.menucommand_separator.12x12.png" width="12" height="12" />
    <icon src="controls_winjs.ui.menucommand_separator.16x16.png" width="16" height="16" />
  </icons>
  <description type="text/plain">
    <![CDATA[Command Separator for use with the WinJS.UI.Menu.]]>
  </description>
  <requires>
    <require type="javascript" src="//Microsoft.WinJS.1.0/js/base.js" />
    <require type="javascript" src="//Microsoft.WinJS.1.0/js/ui.js" />
    <require type="javascript"><![CDATA[WinJS.UI.processAll();]]></require>
  </requires>
  <content>
    <![CDATA[
        <hr data-win-control="WinJS.UI.MenuCommand" data-win-options="{type:'separator'}" />
    ]]>
  </content>
</widget>
